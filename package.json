{
  "name": "bower2nix"

, "version": "2.1.0"

, "description": "Generate nix expressions to fetch bower dependencies"

, "homepage": "https://bitbucket.org/shlevy/bower2nix"

, "author": {
    "name": "Shea Levy"
  , "email": "shea@shealevy.com"
  }

, "bin": { "bower2nix": "lib/command.js" }

, "dependencies": {
    "temp": "0.6.0"
  , "fs.extra": ">=1.2.1 <2"
  , "bower-json": "0.4.0"
  , "bower-endpoint-parser": "0.2.1"
  , "bower-logger": "0.2.1"
  , "bower": ">=1.2.8 <2"
  , "argparse": "0.1.15"
  , "clone": "0.1.11"
  , "semver": ">=2.2.1 <3"
  , "fetch-bower": ">=2 <3"
  }

, "devDependencies": {
    "coffee-script": ">=1.6.3 <2.0.0"
  }

, "repository": {
    "type": "hg"
  , "url": "https://bitbucket.org/shlevy/bower2nix"
  }

, "licenses": [ {
    "type": "MIT"
  , "url": "https://bitbucket.org/shlevy/bower2nix/raw/default/LICENSE"
  } ]

, "scripts": {
    "prepublish": "coffee --bare --compile --output lib src/*.coffee"
  }
}
